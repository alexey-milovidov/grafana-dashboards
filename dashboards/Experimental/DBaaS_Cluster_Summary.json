      "targets": [
        {
          "datasource": "Metrics",
          "editorMode": "code",
          "exemplar": true,
          "expr": "sum (rate (container_cpu_usage_seconds_total{id=\"/\",node=~\"$node_name\"}[$interval])) / sum (machine_cpu_cores{node=~\"$node_name\"}) * 100",
          "interval": "",
          "legendFormat": "CPU Usage%",
          "range": true,
          "refId": "CPU"
        },
        {
          "datasource": "Metrics",
          "editorMode": "code",

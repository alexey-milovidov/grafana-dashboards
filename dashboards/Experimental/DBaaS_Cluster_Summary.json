        {
          "datasource": "Metrics",
          "editorMode": "code",
          "expr": "sum(kube_pod_container_resource_limits{unit=\"byte\",node=~\"$node_name\"}) / sum(machine_memory_bytes{node=~\"$node_name\"}) * 100",

          "legendFormat": "CPU Request %",
          "range": true,
          "refId": "A"
        },
        {
          "datasource": "Metrics",
          "editorMode": "code",
          "expr": "sum(kube_pod_container_resource_limits{unit=\"core\",node=~\"$node_name\"}) / sum(machine_cpu_cores{node=~\"$node_name\"}) * 100",
          "hide": false,
          "legendFormat": "CPU Limit %",
          "range": true,
          "refId": "B"
        },
        {
          "datasource": "Metrics",
          "editorMode": "code",
          "expr": "sum(kube_pod_container_resource_requests{unit=\"byte\",node=~\"$node_name\"}) / sum(machine_memory_bytes{node=~\"$node_name\"}) * 100",
          "hide": false,
          "legendFormat": "Memory Requests % ",
          "range": true,
          "refId": "C"
